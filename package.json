{
  "name": "ember-add-in-repo-tests",
  "version": "1.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-add-in-repo-tests/ember-add-in-repo-tests.git"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "yarn lint && mocha test"
  },
  "dependencies": {
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-source": "^1.1.0"
  },
  "devDependencies": {
    "broccoli": "^1.1.4",
    "broccoli-cli": "^1.0.0",
    "broccoli-test-helper": "^1.2.0",
    "chai": "*",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.1.7",
    "mocha": "*",
    "prettier": "^2.4.1",
    "release-it": "^14.2.1",
    "release-it-lerna-changelog": "^3.1.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "12.22.6",
    "yarn": "1.22.11"
  }
}
